;; tabbar setttings

(set-face-attribute
 'tabbar-default nil
 :family "Monaco"
 :background "#474747"
 :foreground "#474747"
 :box '(:line-width 3 :color "#474747" :style nil)
 ;; :height 0.8
 )

(set-face-attribute
 'tabbar-unselected nil
 :background "#474747"
 :foreground "white"
 :box '(:line-width 3 :color "#474747" :style nil))

(set-face-attribute
 'tabbar-selected nil
 :background "#6b6b6b"
 :foreground "#a6e22e"
 :box '(:line-width 3 :color "#6b6b6b"))

(set-face-attribute
 'tabbar-button nil
 :box nil)

(set-face-attribute
 'tabbar-modified nil
 :background "#6b6b6b"
 :foreground "white"
 :box '(:line-width 3 :color "#6b6b6b"))

(set-face-attribute
 'tabbar-separator nil
 :background "#3e3d31"
 :foreground "#3e3d31"
 :box '(:line-width 1 :color "#3e3d31" :style nil))
